{
	"name": "static",
	"version": "1.0.0",
	"description": "The JS Web Assets.",
	"main": "index.js",
	"author": "Xenio Systems",
	"scripts": {
		"dev": "npm-run-all -p dev:start dev:wds",
		"dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
		"dev:wds": "cross-env NODE_ENV=development webpack-dev-server --progress --mode development --config webpack.config.dev.babel.js",
		"prod": "yarn prod:build && yarn prod:start",
		"prod:analyze": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=analyze webpack -p --progress --mode production --config webpack.config.analyze.babel.js",
		"prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress --mode production --config webpack.config.prod.babel.js",
		"prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
		"prod:stop": "pm2 delete server",
		"lint": "eslint src webpack.config.development.babel.js --ext .js,.jsx",
		"lint-fix": "prettier --single-quote --write \"{,!(dist|flow-typed|lib)/**}/*.{js,jsx}\" && yarn lint --fix",
		"test": "jest --coverage",
		"watch": "webpack-cli --progress --mode development --config webpack.config.dev.babel.js --watch"
	},
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-core": "^6.25.0",
		"babel-eslint": "^8.0.3",
		"babel-jest": "^22.4.1",
		"babel-loader": "^7.0.0",
		"babel-plugin-flow-react-proptypes": "^21.0.0",
		"babel-plugin-import": "^1.7.0",
		"babel-preset-env": "^1.5.2",
		"babel-preset-flow": "^6.23.0",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"babel-preset-stage-3": "^6.24.1",
		"cross-env": "^5.1.4",
		"eslint": "^4.13.1",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-plugin-compat": "^2.1.0",
		"eslint-plugin-flowtype": "^2.34.0",
		"eslint-plugin-import": "^2.2.0",
		"eslint-plugin-jsx-a11y": "^6.0.2",
		"eslint-plugin-react": "^7.0.1",
		"flow-bin": "^0.67.1",
		"jest": "^22.4.2",
		"nodemon": "^1.11.0",
		"npm-run-all": "^4.0.2",
		"pm2": "^2.5.0",
		"postcss": "^6.0.4",
		"postcss-cssnext": "^3.0.2",
		"postcss-loader": "^2.0.6",
		"precss": "^2.0.0",
		"prettier": "^1.11.1",
		"react-svg-loader": "^2.1.0",
		"rimraf": "^2.6.1",
		"style-loader": "^0.20.3",
		"stylefmt": "^6.0.0",
		"stylelint": "^9.1.1",
		"uglifyjs-webpack-plugin": "^1.2.3",
		"webpack": "^4.1.1",
		"webpack-bundle-analyzer": "^2.11.1",
		"webpack-cli": "^2.0.11",
		"webpack-dev-server": "^3.1.1",
		"webpack-merge": "^4.1.2"
	},
	"dependencies": {
		"antd": "^3.4.1",
		"axios": "^0.18.0",
		"babel-polyfill": "^6.26.0",
		"compression": "^1.6.2",
		"express": "^4.15.3",
		"lodash": "^4.17.5",
		"moment": "^2.22.1",
		"prop-types": "^15.6.1",
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"react-helmet": "^5.1.3",
		"react-hot-loader": "^4.0.0",
		"react-redux": "^5.0.7",
		"react-router": "^4.1.1",
		"react-router-dom": "^4.1.1",
		"react-virtualized": "^9.18.5",
		"redux": "^4.0.0",
		"redux-thunk": "^2.2.0",
		"styled-components": "^1.4.6",
		"victory": "^0.25.7"
	}
}
